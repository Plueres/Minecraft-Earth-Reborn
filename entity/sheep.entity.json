{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:sheep",
      "min_engine_version": "1.8.0",
      "materials": {
        "default": "sheep",
        "transparent": "skeleton"
      },
      "textures": {
        "default": "textures/entity/sheep/sheep",
        "horns": "textures/entity/sheep/horns",
        "none": "textures/entity/llama/decor/decor_none",
        "flecked": "textures/entity/sheep/flecked_sheep",
        "fuzzy": "textures/entity/sheep/fuzzy_sheep",
        "inky": "textures/entity/sheep/inky_sheep",
        "long_nosed": "textures/entity/sheep/long_nosed_sheep",
        "patched": "textures/entity/sheep/patched_sheep",
        "rainbow": "textures/entity/sheep/rainbow_sheep",
        "rainbow_wool": "textures/entity/sheep/rainbow_sheep_wool",
        "rocky": "textures/entity/sheep/rocky_sheep"
      },
      "geometry": {
        "default": "geometry.sheep.v1.8",
        "sheared": "geometry.sheep.sheared.v1.8",
        "horns" : "geometry.sheep.horned.v1.8",
        "rainbow_sheep" : "geometry.rainbow_sheep.v1.8"
      },
      "animations": {
        "setup": "animation.sheep.setup",
        "grazing": "animation.sheep.grazing",
        "walk": "animation.quadruped.walk",
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.sheep.baby_transform",
        "move": "controller.animation.sheep.move"
      },
      "scripts": {
        "animate": [
          "setup",
          "look_at_target",
          "move",
          {
            "baby_transform": "query.is_baby"
          }
        ],
        "initialize": [
          "variable.overlay_color = 0;",
          "variable.Svar = Math.random(0,8);"
        ]
      },
      "render_controllers": [
        "controller.render.sheep",
        "controller.render.sheep_horned",
        "controller.render.sheep_rainbow"
      ]
    }
  }
}