{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:pig",
      "min_engine_version": "1.8.0",
      "materials": {
        "default": "pig",
        "transparent": "skeleton"
      },
      "textures": {
        "default": "textures/entity/pig/pig",
        "saddled": "textures/entity/pig/pig_saddle",
        "muddy": "textures/entity/pig/pig_muddy",
        "none": "textures/entity/llama/decor/decor_none",
        "dried_muddy": "textures/entity/pig/pig_dried_muddy",
        "molted": "textures/entity/pig/pig_molted",
        "pale": "textures/entity/pig/pig_pale",
        "piebald": "textures/entity/pig/pig_piebald",
        "pink_footed": "textures/entity/pig/pig_pink_footed",
        "sooty": "textures/entity/pig/pig_sooty",
        "spotted": "textures/entity/pig/pig_spotted"
      },
      "geometry": {
        "default": "geometry.pig.v1.8",
        "muddy_top" : "geometry.pig_muddy.v1.8",
        "saddled": "geometry.pig_saddle.v1.8"
      },
      "animations": {
        "setup": "animation.pig.setup",
        "walk": "animation.quadruped.walk",
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.pig.baby_transform"
      },
      "scripts": {
        "animate": [
          "setup",
          {
            "walk": "query.modified_move_speed"
          },
          "look_at_target",
          {
            "baby_transform": "query.is_baby"
          }
        ],
        "initialize": [
          "variable.Svar = Math.random(0,10);"
        ]
      },
      "render_controllers": [
        "controller.render.pig",
        "controller.render.pig_muddy",
        "controller.render.pig_saddle"
      ]
    }
  }
}