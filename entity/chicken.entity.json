{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:chicken",
      "min_engine_version": "1.12.0",
      "materials": {
        "default": "chicken_legs"
      },
      "textures": {
        "default": "textures/entity/chicken/chicken",
        "amber": "textures/entity/chicken/amber_chicken",
        "bronzed": "textures/entity/chicken/bronzed_chicken",
        "cluckshroom": "textures/entity/chicken/cluck_shroom",
        "fancy": "textures/entity/chicken/fancy_chicken",
        "gold_crested": "textures/entity/chicken/gold_crested_chicken",
        "midnight": "textures/entity/chicken/midnight_chicken",
        "skewbald": "textures/entity/chicken/skewbald_chicken",
        "stormy": "textures/entity/chicken/stormy_chicken"
      },
      "geometry": {
        "default": "geometry.chicken.v1.12",
        "accessories": "geometry.chicken_accessories.v1.12"
      },
      "animations": {
        "move": "animation.chicken.move",
        "general": "animation.chicken.general",
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.chicken.baby_transform"
      },
      "scripts": {
        "animate": [
          "general",
          {
            "move": "query.modified_move_speed"
          },
          "look_at_target",
          {
            "baby_transform": "query.is_baby"
          }
        ],
        "initialize": [
          "variable.Svar = Math.random(0,8);"
        ]
      },
      "render_controllers": [
        "controller.render.chicken",
        "controller.render.chicken_accessories"
      ]
    }
  }
}